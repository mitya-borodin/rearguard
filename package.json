    "@types/jest": "^24.0.21",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.12.5",
