{
  "name": "rearguard",
  "version": "2.4.2",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "lib",
    "templates"
  ],
  "bin": {
    "rearguard": "./lib/bin/rearguard.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://github.com/mitya-borodin-2/rearguard.git",
  "bugs": {
    "url": "https://github.com/mitya-borodin-2/rearguard/issues"
  },
