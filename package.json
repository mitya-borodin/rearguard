{
  "name": "rearguard",
  "version": "5.0.0",
  "engines": {
    "node": ">=10 <11",
    "npm": ">=6 <7"
  },
  "files": [
    "lib",
    "templates",
    "docs"
  ],
  "bin": {
    "rearguard": "./lib/bin/rearguard.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://gitlab.com/mitya-borodin/rearguard",
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/rearguard/issues"
