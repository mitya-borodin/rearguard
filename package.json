  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/rearguard.git"
  },
  "author": {
    "name": "Dmitriy Borodin",
    "email": "dmitriy@borodin.site",
    "url": "http://borodin.site"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "npm run lint && tsc",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts' 'bin/**/*.ts' 'test/**/*.ts'",
    "templates": "node ./tools/copyTemplateFiles.js",
    "make-prettier": "prettier --write '**/*.ts'",
    "prepublish": "npm run templates",
    "prestart": "npm run templates"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run templates"
    }
  },
