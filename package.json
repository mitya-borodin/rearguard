    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.18.2",
    "prettier-package-json": "^2.1.0",
    "raw-loader": "^3.1.0",
    "recursive-copy": "^2.0.10",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "semver": "^6.3.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
