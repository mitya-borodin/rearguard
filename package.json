    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy": "^0.3.2",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "delay": "^4.3.0",
    "ejs": "^2.7.1",
    "eslint-loader": "^3.0.2",
    "execa": "^3.2.0",
    "file-loader": "^4.2.0",
    "find-process": "^1.4.2",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "glob": "^7.1.5",
