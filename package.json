    "isomorphic-style-loader": "^5.1.0",
    "loader-utils": "^1.2.3",
    "lodash.merge": "^4.6.2",
    "lodash.snakecase": "^4.1.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
